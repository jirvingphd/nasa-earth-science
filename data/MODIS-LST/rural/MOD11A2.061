    <script>
        $("#citation_style").change(function(event){
            var citation_type = event.target.value;
            if(citation_type != "default")
            {
                // Give some immediate feedback.  TODO: spinner?
                $("#citation_result").text("Please wait...");

                // make the call and replace the text
                endpoint = 'https://citation.cr.usgs.gov/10.5067/MODIS/MOD11A2.061/' + citation_type + '/';
                $.ajax({
                    url:     endpoint,
                    success: function(response){$("#citation_result").html(response);},
                    error:   function(err){$("#citation_result").html("An error occurred: " + err.responseText);},
                });
            }
            else
            {
                $("#citation_result").text("")
            }
        })
    </script>
    <script>   
        function copyFunction(id) {
            var select = document.getElementById("citation_style").selectedIndex;
	    var str = document.getElementById(id).innerHTML;
	    function listener(e){
 		e.clipboardData.setData("text/html", str);
   		e.clipboardData.setData("text/plain", str);
   		e.preventDefault();
  		}
 	    document.addEventListener("copy", listener);
  	    document.execCommand("copy");
  	    document.removeEventListener("copy", listener);
		
            document.getElementById("myTooltip").style.visibility = "visible";
            if (select == 0){
                var tooltip = document.getElementById("myTooltip");
                tooltip.innerHTML = "Please select a Citation Style...";
            } else {
                var tooltip = document.getElementById("myTooltip");
                tooltip.innerHTML = "Citation Copied";
            }
        }
        function outFunc() {
            var tooltip = document.getElementById("myTooltip");
            tooltip.innerHTML = "";
            document.getElementById("myTooltip").style.visibility = "hidden";
        }
    </script>

        

